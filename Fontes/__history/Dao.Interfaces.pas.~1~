unit Dao.Interfaces;

interface

uses System.Generics.Collections, Data.DB;

type
  iDao<T> = interface
    ['{B28BC29D-B93C-421E-A052-16E17C931C87}']
    function Listar(value : T) : TDataSet;
    procedure Inserir(value : T);
    procedure Alterar(value : T);
    procedure Excluir(value : integer);
  end;


implementation

end.
